{
    "name": "atikrahman/laravel-server-health",
    "description": "This package generates url to check laravel hosted os and server information as well as watch for high alerts",
    "license": "MIT",
    "authors": [
        {
            "name": "atik",
            "email": "atikrahman.ew@gmail.com"
        }
    ],
    "require": {
        "php": "^7.0",
        "illuminate/support": "~5.3 || ^6.0 || ^7.0 || ^8.0"
    },
    "autoload": {
        "psr-4": {
            "atikrahman\\ServerHealth\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "atikrahman\\ServerHealth\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "atikrahman\\ServerHealth\\Providers\\MonitorServiceProvider"
            ]
        }
    }
}
